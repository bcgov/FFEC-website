                <% } %>
                </div>
                -->
        </div>
        <div class="metadata">
            <!-- <a href="<%- item.path %>"> -->

            <!-- </a> -->
            <% if (item.url_source) { %>
            <div class="listing-pub-url">

                    <a href="<%- item.url_source %>">
                    <p style="color:#1a5a96;">URL <i class="bi bi-box-arrow-up-right"></i></p>
                    </a>
                </div>
                    <% } %>


        </div>
    </div>

    <% } %>
    </div>

